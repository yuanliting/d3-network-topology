{
  "name": "vue-create-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "d3": "^7.9.0",
    "dagre-d3": "^0.6.4",
    "element-ui": "^2.15.14",
    "jquery": "^3.7.1",
    "sass": "^1.75.0",
    "svg-sprite-loader": "^6.0.11",
    "vue": "^2.6.14",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/d3": "^7.4.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.7.1",
    "node-sass": "^9.0.0",
    "sass-loader": "^14.2.1",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unsafe-finally": "off",
      "no-mixed-spaces-and-tabs": 0,
      "vue/multi-word-component-names": 0,
      "no-useless-escape": 0,
      "no-debugger": 0,
      "no-unused-vars": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
